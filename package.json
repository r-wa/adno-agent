{
  "name": "adno-agent",
  "version": "1.0.3",
  "description": "Background processing agent for adno workspace automation",
  "main": "dist/index.js",
  "bin": {
    "adno-agent": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "type-check": "tsc --noEmit",
    "package": "pkg dist/index.js --targets node18-win-x64 --output adno-agent-windows-x64.exe"
  },
  "pkg": {
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/r-wa/adno-agent"
  },
  "keywords": [
    "adno",
    "agent",
    "automation",
    "background-worker"
  ],
  "author": "Ryan W",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.7",
    "pino": "^8.21.0",
    "rotating-file-stream": "^3.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.17.11",
    "pino-pretty": "^10.3.1",
    "pkg": "^5.8.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
